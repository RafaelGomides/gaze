{{ template "header" }}

  <dialog class="mdl-dialog">
    <h5 class="mdl-dialog__title">Size</h5>
    <div class="mdl-dialog__content">
      <form action="#">
				<div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
					<input class="mdl-textfield__input" type="text" pattern="-?[0-9]*(\.[0-9]+)?" id="height" value="20">
					<label class="mdl-textfield__label" for="height">Height...</label>
					<span class="mdl-textfield__error">Input is not a number!</span>
				</div>
				<div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
					<input class="mdl-textfield__input" type="text" pattern="-?[0-9]*(\.[0-9]+)?" id="width" value="40">
					<label class="mdl-textfield__label" for="height">Width...</label>
					<span class="mdl-textfield__error">Input is not a number!</span>
				</div>
			</form>
    </div>
    <div class="mdl-dialog__actions">
      <button type="button" class="mdl-button">Ok</button>
      <button type="button" class="mdl-button close">Cancel</button>
    </div>
  </dialog>

	<table>
		{{ range $row := .Cells}}
		<tr>
				{{ range $cell := $row}}
					<td class="{{$cell.CssClasses}}"></td>
				{{ end }}
		</tr>
		{{ end}}
	</table>

{{ template "footer" }}
